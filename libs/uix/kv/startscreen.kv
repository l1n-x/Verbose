# -*- coding: utf-8 -*-
#:import MDToolbar kivymd.toolbar
#:import MDIconButton kivymd.button.MDIconButton
#:import NoTransition kivy.uix.screenmanager.NoTransition
#:import Login libs.uix.baseclass.login.Login
#:import Registration libs.uix.baseclass.registration.Registration
#:import Corresp libs.uix.baseclass.corresp.Corresp
#:import Profile libs.uix.baseclass.profile.Profile
#:import Dialogs libs.uix.baseclass.dialogs.Dialogs
#:import SettingsScreen libs.uix.baseclass.settings.SettingsScreen
#:import License libs.uix.baseclass.license.License
#:import About libs.uix.baseclass.about.About

<StartScreen>
    orientation: 'vertical'
    MDToolbar:
        id: action_bar
        background_color: app.theme_cls.primary_color
        title: app.title
        elevation: 10
        md_bg_color: app.theme_cls.primary_color
    ScreenManager:
        transition:NoTransition()
        id: manager

        Profile:
            id: profile
            
        Login:
            id: login

        Registration:
            id: registration

        

        Dialogs:
            id: dialogs

        Corresp:
            id: corresp

        SettingsScreen:
            id: settings

        License:
            id: license

        About:
            id: about
